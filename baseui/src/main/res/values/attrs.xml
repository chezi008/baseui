<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TabLayoutView">
        <attr name="textColor" format="color" />
    </declare-styleable>

    <declare-styleable name="BaseCell">
        <attr name="cell_show_underline" format="boolean" />
        <attr name="cell_title" format="string" />
        <attr name="cell_title_style" format="reference" />
        <attr name="cell_center_icon" format="reference" />
        <attr name="cell_left_content" format="string" />
        <attr name="cell_left_content_style" format="reference" />
        <attr name="cell_right_content" format="string" />
        <attr name="cell_right_content_style" format="reference" />
        <attr name="cell_left_icon" format="reference" />
        <attr name="cell_left_icon_dimen" format="dimension" />
        <attr name="cell_right_icon" format="reference" />
        <attr name="cell_right_icon_dimen" format="dimension" />

    </declare-styleable>

    <declare-styleable name="TitleBar">
        <attr name="titleLeftText" format="string" />
        <attr name="titleLeftImage" format="reference" />
        <attr name="titleCenterText" format="string" />
        <attr name="titleRightText" format="string" />
        <attr name="titleRightImage" format="reference" />
        <attr name="titleBgColor" format="color" />
        <attr name="titleTextColor" format="color" />
        <attr name="titleLeftGone" format="boolean" />
    </declare-styleable>
</resources>